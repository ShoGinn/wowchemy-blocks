    "cspell": "^6.12.0",
    "html-validate": "^7.5.0",
    "html-validate-markdown": "^3.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-go-template": "^0.0.13",
    "rimraf": "^3.0.2",
    "run-script-os": "^1.1.6"
